def shop():
    items = {"carrot":0,25,"bread" :1.55, "eggs" : 2.99, "milk":1.59}
    return items

def view all (products={}):
    for x,y in products:
         print(f"{x}-------{y}")

def playing():
    d = shop()
    d["cheese"] =5.99
    d["milk"] = 1.99
    del d["eggs"]
    prod = input("Enter new product:")
    price =float(input("Enter it's price"))
    d[prod] = price
    view_all(d)
playing()